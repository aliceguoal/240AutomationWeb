    <servlet-name>ToolGroupConsumptionServlet</servlet-name>
    <url-pattern>/ToolGroupConsumptionServlet</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>ToolConsumptionServlet</servlet-name>
