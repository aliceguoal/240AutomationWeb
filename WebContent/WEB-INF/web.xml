  </servlet>
  <servlet-mapping>
    <servlet-name>ToolConsumptionServlet</servlet-name>
    <url-pattern>/ToolConsumptionServlet</url-pattern>
  </servlet-mapping>
  
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
</web-app>